---
{
    "stream": {
        "$id": "faq-pages",
        "source": "knowledgeGraph",
        "destination": "pages",
        "filter" : {
            "entityTypes": [
                "faq"
            ]
        },
        "fields": [
            "id", 
            "uid", 
            "meta",
            "question",
            "answer",
            "slug"
        ], 
        "localization": {
            "locales": ["en"]
        }
    },
    "urlFormat": "{{slug}}"
}
---
<!DOCTYPE html>
<html lang="en">
    <head>
        {{>head }}
        <title>QC Holdings FAQs - {{question}}</title>
